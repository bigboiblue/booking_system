{
    "env": {
        "defaultIncludePath": [
            "${workspaceFolder}/frontend",
            "${workspaceFolder}/frontend/include",
        ]
    },
    "configurations": [
        {
            "name": "linux",
            "intelliSenseMode": "${default}",
            "includePath": [
                "${defaultIncludePath}",
                "/usr/include/",
                "/usr/include/qt"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}/frontend"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}